{
  "name": "pupt-lib",
  "version": "0.0.0-development",
  "description": "TypeScript library for creating AI prompts using JSX syntax",
  "license": "MIT",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "import": "./dist/jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-runtime/jsx-dev-runtime.js"
    },
    "./babel-preset": {
      "types": "./dist/babel/preset.d.ts",
      "import": "./dist/babel/preset.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --no-coverage --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "knip": "knip",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.19.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/node": "^22.10.10",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "knip": "^5.44.1",
    "pupt": "^1.4.1",
    "semantic-release": "^24.2.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.4"
  }
}

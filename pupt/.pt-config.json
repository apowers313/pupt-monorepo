{
  "promptDirs": [
    "./prompts"
  ],
  "historyDir": "./.pthistory",
  "annotationDir": "./.pthistory",
  "version": "7.0.0",
  "defaultCmd": "claude",
  "defaultCmdArgs": [
    "--verbose",
    "--permission-mode",
    "acceptEdits"
  ],
  "defaultCmdOptions": {
    "Continue with last context?": "--continue"
  },
  "autoReview": true,
  "autoRun": true,
  "gitPromptDir": ".git-prompts",
  "outputCapture": {
    "enabled": true,
    "directory": "./.pthistory",
    "maxSizeMB": 150
  },
  "libraries": [],
  "environment": {
    "llm": {
      "provider": "anthropic"
    },
    "code": {
      "language": "typescript"
    }
  }
}
